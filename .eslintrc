{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", "tab"],
        "no-console": "error",
        "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
        "yoda": "error"
    }
}